{
  "expo": {
    "name": "RunPacer",
    "slug": "runpacer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/runpacer_icon.png",
    "scheme": "runpacer",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/runpacer_icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.littlepodang.runpacer"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/runpacer_splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6c00b77b-f75b-4a72-b899-3f039cf4fcf6"
      }
    },
    "owner": "fo_rdang"
  }
}
